{
	"name": "My EDiary Tweaks",
	"version": "0.6.0",
	"description": "A Google Chrome extension designed to improve the UI of the My EDiary student diary app.",
	"manifest_version": 2,

	"content_scripts": [
		{
			"matches": ["https://*.myediary.com.au/diary_app/*"],
			"js": ["js/early.min.js"],
			"run_at": "document_end"
		},
		{
			"matches": ["https://*.myediary.com.au/diary_app/*"],
			"js": ["js/late.min.js"]
		}
	],
	"icons": {
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"web_accessible_resources": [
		"css/app.css",
		"css/app.css.map",
		"css/app.min.css",
		"js/early.js",
		"js/early.min.map",
		"js/late.js",
		"js/late.min.map",
		"fonts/*"
	]
}
